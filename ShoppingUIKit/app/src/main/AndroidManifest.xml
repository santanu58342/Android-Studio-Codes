<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.trendingdesign.shoppinguikit">

    <application
        android:allowBackup="true"
        android:icon="@drawable/shopping_bag1"
        android:label="@string/app_name"
        android:roundIcon="@drawable/shopping_bag1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.trendingdesign.shoppinguikit.SplashActivity">
        </activity>
        <activity android:name="com.trendingdesign.shoppinguikit.LoginActivity" />
        <activity android:name="com.trendingdesign.shoppinguikit.SignupActivity" />
        <activity android:name="com.trendingdesign.shoppinguikit.ShoppingActivity" />
        <activity android:name="com.trendingdesign.shoppinguikit.DashboardActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.trendingdesign.shoppinguikit.NavigationActivity"></activity>
    </application>

</manifest>